{
  "name": "@eru/api-tester",
  "version": "1.0.0",
  "description": "API tester cli",
  "bin": {
    "api-tester": "./bin/api-tester.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "eslint-fix": "eslint --fix .",
    "clean": "rimraf ./report ./allure-results ./allure ./lib",
    "testbk": "node ./bin/api-tester.js --testFiles=./example/test/*.test.ts --parallel",
    "test": "npm run clean && tsc && sleep 1 && mocha --config .mocharc.json",
    "allure-report": "allure generate ./allure-results -c && allure open"
  },
  "keywords": [],
  "files": [
    "lib/**/*"
  ],
  "author": "",
  "license": "ISC",
