    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "jsonpath-plus": "^6.0.1",
    "luxon": "^2.0.2",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.0.1",
    "properties-reader": "^2.2.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^8.10.1",
